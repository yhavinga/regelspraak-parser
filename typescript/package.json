{
  "name": "regelspraak",
  "version": "0.2.0",
  "description": "TypeScript implementation of RegelSpraak DSL - Dutch business rules engine",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yhavinga/regelspraak-parser.git",
    "directory": "typescript"
  },
  "homepage": "https://github.com/yhavinga/regelspraak-parser#readme",
  "bugs": {
    "url": "https://github.com/yhavinga/regelspraak-parser/issues"
  },
  "author": "Yeb Havinga <yhavinga@gmail.com>",
  "scripts": {
    "prebuild": "node scripts/extract-multiword.js",
    "build": "tsc && npm run check-size",
    "build:browser": "node scripts/build-for-browser.js",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "check-size": "node scripts/check-module-sizes.js",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "regelspraak",
    "parser",
    "dsl",
    "dutch",
    "business-rules",
    "rules-engine",
    "antlr4"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "esbuild": "^0.19.12",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "antlr4": "^4.13.2",
    "antlr4-c3": "^3.4.4",
    "decimal.js": "^10.4.3"
  }
}
